<Layout.page_content>
  <:header>
    <Layout.header socket={@socket} title={@credential.name || @page_title} />
  </:header>
  <Layout.centered>
    <.live_component
      module={LightningWeb.CredentialLive.FormComponent}
      id={@credential.id || :new}
      title={@page_title}
      action={@live_action}
      credential={@credential}
      projects={@projects}
      return_to={@return_to}
      caller_context={@caller_context}
    />
  </Layout.centered>
</Layout.page_content>
